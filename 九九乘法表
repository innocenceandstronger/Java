//multiplicationTable   乘法表
public class Java练习{
    public static void multiplicationTable(){    
	    for(int i=1;i<10;i++)
		{
		    for(int j=1;j<=i;j++)
			{
			    System.out.printf("%d*%d=%-2d ",j,i,i*j);
			}
			System.out.println();
		}
	}
	public static void main(String[] args){
        multiplicationTable();
    }
}	
