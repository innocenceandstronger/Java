class Person{
    public void print(){
        System.out.println("1.我是爸爸！");
    }
}
class Student extends Person{
    public void print(){
        System.out.println("2.我是儿子！");
    }
}
public class Excipise1112 {
    public static void main(String[] args) {
        Person per = new Student();
        per.print();
    }
}
