import java.util.*;
public class Excipise{
    public static void main(String[] args){
        Scanner input=new Scanner(System.in);
        int n=input.nextInt();
        int[] a=new int[n];
        for(int i=0;i<n;i++){
            a[i]=input.nextInt();
        }
        int grade=input.nextInt();
        int count=0;
        for(int i=0;i<n;i++){
            if(a[i]==grade){
                count++;
            }
        }
        System.out.println(count);
    }
}
