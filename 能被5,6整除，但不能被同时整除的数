public class Test1002 {     //能被5或6整除，但不能被同时整除
    public static void main(String[] args) {
        int count = 0;
        for (int i = 0; i < 100; i++) {
            if (i % 5 == 0 && i % 6 == 0) {
                i++;
            } else if (i % 5 == 0 || i % 6 == 0) {
                System.out.print(i + " ");
                count++;
            }
        }
    }
}
