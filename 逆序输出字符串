import java.util.*;
public class Main{
    public static void main(String[] args){
        Scanner input=new Scanner(System.in);
        String a=input.nextLine();
        String b="";
        char[] c=a.toCharArray();
        for(int i=c.length-1;i>=0;i--){
            b=b+c[i];
        }
        System.out.println(b);
        input.close();
    }
}
